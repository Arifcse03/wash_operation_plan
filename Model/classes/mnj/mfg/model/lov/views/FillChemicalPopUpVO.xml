  <SQLQuery>
    <![CDATA[SELECT LINE_ID,
       (SELECT MSI.CONCATENATED_SEGMENTS
          FROM MTL_SYSTEM_ITEMS_KFV MSI
         WHERE MSI.INVENTORY_ITEM_ID = DD.INVENTORY_ITEM_ID
           AND MSI.ORGANIZATION_ID = 353) CODE,
       INVENTORY_ITEM_ID, UOM, PER_BATCH_CONS,
    (select  distinct l.PER_BATCH_QUANTITY from  MNJ_MFG_WASH_RECIPE_L l
           where l.line_id = dd.line_id
           and rownum = 1)PER_BATCH_QTY,
       (SELECT SUM (TRANSACTION_QUANTITY)
          FROM MTL_ONHAND_QUANTITIES MOQ
         WHERE MOQ.INVENTORY_ITEM_ID = DD.INVENTORY_ITEM_ID
           AND MOQ.ORGANIZATION_ID = 357) ON_HAND_QTY,
DD.Total_Consumption total_cons,
 (SELECT MSI.description
          FROM MTL_SYSTEM_ITEMS_KFV MSI
         WHERE MSI.INVENTORY_ITEM_ID = DD.INVENTORY_ITEM_ID
           AND MSI.ORGANIZATION_ID = 353)Item_Desc
  FROM MNJ_MFG_WASH_RECIPE_D DD]]>
  </SQLQuery>
