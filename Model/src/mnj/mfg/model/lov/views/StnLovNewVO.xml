  <SQLQuery>
    <![CDATA[SELECT DISTINCT STN_NO , OOLA.ORDERED_ITEM , OOLA.USER_ITEM_DESCRIPTION , OOLA.ORDERED_QUANTITY ,ORDERED_ITEM_ID,
STYLE_NAME,WASH
               FROM MNJ_MFG_WASH_RECIPE_H MWRH ,
                    OE_ORDER_HEADERS_ALL  OOHA , 
                    OE_ORDER_LINES_ALL    OOLA
              WHERE OOHA.HEADER_ID  = OOLA.HEADER_ID 
                AND MWRH.STN_NO     = OOHA.CUST_PO_NUMBER||'/'||LINE_NUMBER
AND NOT EXISTS (SELECT * FROM MNJ_GWP_MFG_GE_OBSLINE mmgp WHERE mmgp.obs_no = MWRH.STN_NO)]]>
